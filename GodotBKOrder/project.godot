; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="CF Digital Order"
run/main_scene="res://node_2d.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Data"
config/features=PackedStringArray("4.3")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://cfd-LOGO-transparent2048x2048.png"
config/icon="res://cfd-LOGO-transparent300x300.png"
config/windows_native_icon="res://cfd-LOGO-transparent300x300.png"

[display]

window/size/viewport_width=1600
window/size/viewport_height=800
window/dpi/allow_hidpi=false

[editor_plugins]

enabled=PackedStringArray("res://addons/excel_reader/plugin.cfg")

[input]

open_file={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":79,"key_label":0,"unicode":111,"location":0,"echo":false,"script":null)
]
}

[rendering]

renderer/rendering_method="gl_compatibility"
